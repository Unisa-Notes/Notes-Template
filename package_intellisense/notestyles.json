    "rulechapterend": {}
  },
  "envs": {
    "definition{}": {
      "name": "definition",
      "snippet": "{${1:title}}"
    },
    "example": {},
    "exercise{}": {
      "name": "exercise",
      "snippet": "{${1:title}}"
    },
    "indentparagraph": {},
    "sidenote{}": {
      "name": "sidenote",
      "snippet": "{${1:title}}"
    },
    "descriptimize": {
      "name": "descriptimize",
      "snippet": "\n\t\\item[$1] $0"
    }
  },
  "options": []
}