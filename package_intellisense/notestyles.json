  "cmds": {
    "addfile[]{}": {
      "snippet": "addfile[${2:input}]{${1:<filename>}}"
    },
    "addfile{}": {
      "snippet": "addfile{${1:<filename>}}"
    },
    "concept{}": {
      "snippet": "concept{${1:text}}"
    },
